{
    "name": "calTally",
    "version":"0.1",
    "description": "counts the number of hours attached to each calendar",
    "permissions":[
        "storage",
        "declarativeContent",
        "activeTab",
        "identity"
    ],
    // scripts to use
    "background":{
        "scripts":[
            "background.js"
        ],
        "persistent": false
    },
    // html pages
    "options_page": "options.html",
    "page_action":{
        "default_popup": "popup.html",
        //toolbar icons
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
          }
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      },
      "oauth2": {
        "client_id": "846512348341-qmn0uhc2u3i0k4d23tbk5moet2fjilfb.apps.googleusercontent.com",
        "scopes": [
          "https://www.googleapis.com/auth/calendar.readonly"
        ]
      },
    "manifest_version": 2
}